//pogram to delete element at specific position
#include<stdio.h>
void main()
{
    int a[10],i,num,n,pos;
    printf("enter size of array");
    scanf("%d",&n);
    printf("enter array elements");
    for(i=0;i<n;i++)
    {
    scanf("%d",&a[i]);
    }
    printf("enter position to delete element");
    scanf("%d",&pos);
    for(i=pos-1;i<n-1;i++){
        a[i]=a[i+1];
    }
for(i=0;i<n-1;i++){
    printf("%d",a[i]);
}
}
